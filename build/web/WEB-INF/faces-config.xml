<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean>
        <managed-bean-name>produtorBean</managed-bean-name>
        <managed-bean-class>dust.web.ProdutorBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
        <managed-bean>
        <managed-bean-name>coletaBean</managed-bean-name>
        <managed-bean-class>dust.web.ColetaBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
   
    <navigation-rule>

    <from-view-id>*</from-view-id>
    
    <navigation-case>
        <from-outcome>inicio</from-outcome>
        <to-view-id>/index.xhtml</to-view-id>
    </navigation-case>
    
      <navigation-case>
        <from-outcome>cadastroProdutor</from-outcome>
        <to-view-id>/cadastroProdutor.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
        <from-outcome>cadastroColeta</from-outcome>
        <to-view-id>/cadastroColeta.xhtml</to-view-id>
    </navigation-case>



</navigation-rule>

<navigation-rule>
    <navigation-case>
        <from-view-id>/cadastroProdutor.xhtml</from-view-id>
        
        <from-outcome>sucesso</from-outcome>
        <to-view-id>/mostraProdutor.xhtml</to-view-id>
    </navigation-case>
    
        <navigation-case>
        <from-view-id>/cadastroColeta.xhtml</from-view-id>
        
        <from-outcome>sucessoColeta</from-outcome>
        <to-view-id>/mostraColeta.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
    
</faces-config>
