<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean>
        <managed-bean-name>produtorBean</managed-bean-name>
        <managed-bean-class>dust.web.ProdutorBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>recicladorBean</managed-bean-name>
        <managed-bean-class>dust.web.RecicladorBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
   
    <navigation-rule>

        <from-view-id>*</from-view-id>
    
        <navigation-case>
            <from-outcome>inicio</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    
        <navigation-case>
            <from-outcome>cadastroProdutor</from-outcome>
            <to-view-id>/cadastroProdutor.xhtml</to-view-id>
        </navigation-case>
        
         <navigation-case>
            <from-outcome>cadastroReciclador</from-outcome>
            <to-view-id>/cadastroReciclador.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>


    <navigation-rule>
        
        <navigation-case>
            <from-view-id>/cadastroProdutor.xhtml</from-view-id>
            <from-outcome>sucesso</from-outcome>
            <to-view-id>/mostraProdutor.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    
    
    <navigation-rule>
        
        <navigation-case>
            <from-view-id>/cadastroReciclador.xhtml</from-view-id>
            <from-outcome>sucesso</from-outcome>
            <to-view-id>/mostraReciclador.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    
    
</faces-config>
